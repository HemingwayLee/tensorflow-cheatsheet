    "import tensorflow as tf\n",
    "\n",
    "#a 1x2 matrix\n",
    "a = tf.constant([[3., 3.]])\n",
    "\n",
    "#a 2x1 matrix\n",
    "b = tf.constant([[2.],[2.]]) \n",
    "\n",
    "product = tf.matmul(a, b)\n",
    "\n",
    "sess = tf.Session()\n",
    "print(\"sess.run(a*b): \", sess.run(product))"
   ]
  }
